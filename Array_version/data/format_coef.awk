#! /bin/awk -f
{
    if((NR-1)%5 == 0)
    {
        var = -$3/(3.0*$2);
        if(var < 0)
            var = 0;
        if($2 > 0)
            #printf "%.1f & %.4e & %.4e & %.4f & %.4f & [%.2f, $\\infty$) \\\\ \n", $1, $2, $3, $4, $5, var;
            printf "%.1f & %.4e & %.4e & %.4f & %.4f & $\\ge$%.2f \\\\ \n", $1, $2, $3, $4, $5, var;
        else
            #printf "%.1f & %.4e & %.4e & %.4f & %.4f & [0,%.2f]\\\\ \n", $1, $2, $3, $4, $5, var;
            printf "%.1f & %.4e & %.4e & %.4f & %.4f & $\\le$%.2f\\\\ \n", $1, $2, $3, $4, $5, var;
    }
}

